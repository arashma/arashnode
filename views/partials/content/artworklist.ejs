<article class="artworklist">
    <h1>Artwork on display</h1>
    <p>will you attend the confrence head over to our gallery where to checkout some of the work from our speakers</p>
    <div class="pixgrid">

        <% if(artwork.length > 0){ %>
            <% for (var i = 0; i < artwork.length; i++) { %>
                <img src="/images/artwork/<%= artwork[i] %>" alt="Artwork <%= i %>">
            <% } %>
        <% } %>
    </div>

</article>